{
    "name": "edgardhsl/laravel-dblib-odbc-sqlserver",
    "type": "library",
    "description": "PDO DBLIB or PDO ODBC SQL Server Driver for Laravel 4",
    "keywords": [
        "odbc",
        "laravel"
    ],
    "authors": [
        {
            "name": "Takuya Wakisaka",
            "email": "takuya.wakisaka@moldweorp.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.13.0",
        "illuminate/database": "^9.13.0"
    },
    "autoload": {
        "psr-4": {
            "DblibOdbcSqlServer\\Database\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "require-dev": {
        "composer/composer": "2.4.x-dev"
    }
}
